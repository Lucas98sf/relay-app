{
  "name": "@relay-app/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:relay": "relay-compiler",
    "dev:vite": "vite",
    "relay": "relay-compiler --watch",
    "build": "relay-compiler --validate && tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0",
    "babel-plugin-relay": "^14.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.4.0",
    "graphql": "^16.6.0",
    "relay-compiler": "^14.1.0",
    "vite": "^3.1.0",
    "vite-plugin-relay": "^2.0.0"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema/schema.graphql",
    "language": "javascript"
  }
}
